{
  "routes": [
    {
      "route": "/authenticated",
      "methods": [
        "GET",
        "PUT",
        "POST",
        "PATCH",
        "DELETE"
      ],
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
        "route": "/api/secret/*",
        "allowedRoles": [
          "authenticated"
        ]
      }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{png,ico,json}",
      "/static/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}